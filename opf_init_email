#!bin/bash
 
 openproject config:set EMAIL_DELIVERY_METHOD=$EMAIL_DELIVERY_METHOD \
 openproject config:set SMTP_ADDRESS=$SMTP_ADDRESS \
 openproject config:set SMTP_PORT=$SMTP_PORT \
 openproject config:set SMTP_DOMAIN=$SMTP_DOMAIN \
 openproject config:set SMTP_AUTHENTICAITON=$SMTP_AUTHENTICAITON \
 openproject config:set SMTP_USER_NAME=$SMTP_USER_NAME \
 openproject config:set SMTP_PASSWORD=$SMTP_PASSWORD \
 openproject config:set SMTP_ENABLE_STARTTLS_AUTO=$SMTP_ENABLE_STARTTLS_AUTO \
 opf_init
